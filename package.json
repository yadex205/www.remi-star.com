  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "deploy": "netlify deploy --prod --dir=public -m \"$NETLIFY_DEPLOY_MESSAGE\"",
    "lint": "run-s lint:script lint:style",
    "lint:script": "eslint ./src --ext .ts --ext .tsx --color",
    "lint:style": "stylelint 'src/**/*.@(css|scss)'",
    "lint:style:fix": "stylelint --fix 'src/**/*.@(css|scss)'",
    "prettier": "prettier --check './**/*.@(js|ts|tsx|scss|css)'",
    "prettier:fix": "prettier --write './**/*.@(js|ts|tsx|scss|css)'"
  },
  "dependencies": {
    "fibers": "^5.0.0",
    "gatsby": "^3.1.1",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-sass": "^4.1.0",
    "gatsby-source-contentful": "^5.1.1",
    "gatsby-transformer-remark": "^3.1.0",
    "node-sass-glob-importer": "^5.3.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "sass": "^1.32.8",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "netlify-cli": "^3.13.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-hudochenkov": "^5.0.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0"
  },
  "private": true
}
